# Application Configuration
NODE_ENV=development
PORT=3000

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_TTL=300

# For production, set a secure Redis password
# REDIS_PASSWORD=your_secure_redis_password_here

# Logging Configuration
LOG_LEVEL=debug
LOG_FILE_PATH=logs/

# API Configuration
API_PREFIX=api
API_VERSION=v1

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_MAX=100

# Cache Configuration
CACHE_TTL=300
DISTANCE_CACHE_TTL=86400

# Health Check Configuration
HEALTH_CHECK_TIMEOUT=5000
